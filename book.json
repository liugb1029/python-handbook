{
    "title": "Python全栈开发",
    "description": "Python全栈开发",
    "author": "liuguobing",
    "language": "zh-hans",
    "root": ".",

    "plugins": [
        "github-buttons@2.1.0",
        "edit-link",
        "page-treeview",
        "back-to-top-button",
        "theme-comscore",
        "chapter-fold",
        "expandable-chapters",
        "splitter",
        "-lunr", 
        "-search", 
        "search-pro",
        "code"
    ],

    "pluginsConfig": {
        "github-buttons": {
            "repo": "liugb1029/gitbook",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "edit-link": {
            "base": "https://github.com/liugb1029/gitbook/edit/master",
            "label": "Edit This Page"
        },
        "page-treeview": {
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        }
    }
}